    "strip-pragma-loader": "^1.0.1",
    "typescript": "^4.8.2",
    "vue-cli-plugin-vuetify": "~2.5.5",
    "vuetify-loader": "^1.9.2"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "plugins": [
      "vuetify"
    ],
    "extends": [
      "eslint:recommended",
      "plugin:vue/recommended",
      "@vue/airbnb",
      "@vue/typescript"
    ],
    "parserOptions": {
      "ecmaVersion": 2020,
      "parser": "@typescript-eslint/parser"
    },
    "rules": {},
    "overrides": [
      {
        "files": [
          "**/__tests__/*.{j,t}s?(x)",
          "**/tests/unit/**/*.spec.{j,t}s?(x)"
        ],
        "env": {
          "jest": true
        }
      }
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
